+        skip = valid = 1
+        if(s[i] == '('):
+            tok = tok_list[0]
+        elif(s[i] == ')'):
+            tok = tok_list[1]
+        elif(s[i] == '&'):
+            tok = tok_list[2]
+        elif(s[i] == '|'):
+            tok = tok_list[3]
+        elif(s[i] == '!'):
+            tok = tok_list[4]
+        elif(s[i:i + 2] == '->'):
+            tok = tok_list[5]
+            skip = 2
+        elif(s[i:i + 3] == '<->'):
+            tok = tok_list[6]
+            skip = 3
+        
+        if(len(tok) > 0):
+            toks.append(tok)
+            i += skip
+            continue
+        else:
+            #token is a variable name
+            if(s[i] == ' '):
+                i += 1
+                continue
+            
+            while(i < len(s) and s[i] not in operators and s[i] != ' '):
+                tok += s[i]
+                i += 1
+            
+            if(len(tok) > 0):

+        lval = 'False'
+    elif(args[0] == 'True'):
+        lval = 'True'
+    else:
+        lval = vars[args[0]]
+
+    if(args[2] == 'False'):
+        rval = 'False'
+    elif(args[2] == 'True'):
+        rval = 'True'

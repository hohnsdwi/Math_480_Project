+    i = 0
+    while(i < len(lrtoks)):
+        if(lrtoks[i] == 'NOT'):
+            args = [lrtoks[i], lrtoks[i + 1]]
+            lrtoks[i] = eval_mon_op(args)
+            del lrtoks[i + 1]
+        i += 1
